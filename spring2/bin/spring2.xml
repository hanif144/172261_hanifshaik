<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

<bean id ="triangle" class=" com.spring2.han.Triangle">
<property name="pointA" ref="zeropoint"/>



<property name="pointB" ref="pointA"/>
<property name="pointC" ref="pointB"/>

</bean>
<bean id="zeropoint" class="com.spring2.han.Point">
<property name="x" value="0"/>
<property name="y" value="20"/>
</bean>



<bean id="pointA" class="com.spring2.han.Point">
<property name="x" value="0"/>
<property name="y" value="20"/>
</bean>
<bean id="pointB" class="com.spring2.han.Point">
<property name="x" value="0"/>
<property name="y" value="20"/>
</bean>
</beans>