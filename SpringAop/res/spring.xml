<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  



<aop:aspectj-autoproxy />  
		<bean name="triangle" class="com.aop.main.model.Triangle">
		<property name="name" value="triangle name"/>
		</bean>
		
		<bean name="circle" class="com.aop.main.model.Circle">
		<property name="name" value="circle name"/>
		</bean>
		<bean name="shapeservice" class="com.aop.main.service.ShapeService" autowire="byName"/>
		<bean name="loggingAspect" class="com.aop.ascept.LoggingAscept"
		
</beans>